{
  "version": "0.0.0",
  "name": "mongoose-zod",
  "description": "Create Mongoose schemas using Zod.",
  "scripts": {
    "lint:js": "eslint **/*.{js,jsx,csj,mjs,ts,tsx,cts,mts}",
    "lint:js:fix": "eslint **/*.{js,jsx,csj,mjs,ts,tsx,cts,mts} --fix",
    "lint:editorconfig": "editorconfig-checker",
    "lint:types": "tsc --noEmit",
    "lint": "npm run lint:editorconfig && npm run lint:types && npm run lint:js",
    "test:ts": "jest",
    "test": "npm run lint && npm run test:ts",
    "build": "tsup src/index.ts --dts --treeshake",
    "prepublishOnly": "npm run test && npm run build"
  },
  "homepage": "https://github.com/andreww2012/mongoose-zod",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreww2012/mongoose-zod"
  },
  "bugs": {
    "url": "https://github.com/andreww2012/mongoose-zod/issues/new"
  },
  "keywords": [
    "mongoose",
    "zod",
    "mongodb"
  ],
  "author": "andreww2012",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "endOfLine": "lf"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^29.1.1",
    "@types/lodash": "^4.14.186",
    "@types/node": "^16.11.62",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-unicorn": "^44.0.1",
    "jest": "^29.1.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.6.5",
    "mongoose-long": "^0.5.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "zod": "^3.19.1"
  },
  "peerDependencies": {
    "mongoose": "6.x",
    "zod": "^3.x"
  }
}
